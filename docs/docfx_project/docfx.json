{
  "metadata": [
    {
      "src": [
        {
          "src": "../../Subscription.Core",
          "files": [ "Subscription.Core.csproj" ]
        },
        {
          "src": "../../Subscription.Publisher",
          "files": [ "Subscription.Publisher.csproj" ]
        },
        {
          "src": "../../Subscription.Consumer",
          "files": [ "Subscription.Consumer.csproj" ]
        },
        {
          "src": "../../Subscription.Infrastructure",
          "files": [ "Subscription.Infrastructure.csproj" ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "api/index.md" ]
      },
      {
        "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": [
      {
        "files": [ "apidoc/**.md" ],
        "exclude": [ "obj/**", "_site/**" ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
